{
  "_version": "1.59.0",
  "sap.app": {
    "id": "com.ndbs.ui.librarymanegementui",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.12.2",
      "toolsId": "dac7a3f4-9825-49b5-8274-ca07060cfe6f"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v2/library-management/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      },
      "visibilitySource": {
        "uri": "./model/visibility.json",
        "type": "JSON"
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.4",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.ndbs.ui.librarymanegementui.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay"
        }
      },
      "visibilityModel": {
        "dataSource": "visibilitySource",
        "preload": true,
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "defaultBindingMode": "TwoWay"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.ndbs.ui.librarymanegementui.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteHomepage",
          "pattern": "",
          "target": [
            "TargetHomepage"
          ]
        },
        {
          "name": "RouteBookSearch",
          "pattern": "bookSearch",
          "target": [
            "TargetBookSearch"
          ]
        },
        {
          "name": "RouteLibraryManagement",
          "pattern": "libraryManagement",
          "target": [
            "TargetLibraryManagement"
          ]
        }
      ],
      "targets": {
        "TargetHomepage": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Homepage",
          "viewName": "Homepage"
        },
        "TargetBookSearch": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "BookSearch",
          "viewName": "BookSearch"
        },
        "TargetLibraryManagement": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "LibraryManagement",
          "viewName": "LibraryManagement"
        }
      }
    },
    "rootView": {
      "viewName": "com.ndbs.ui.librarymanegementui.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}